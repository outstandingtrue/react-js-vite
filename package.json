{
  "private": true,
  "type": "module",
  "name": "react-js-vite",
  "description": "리액트 템플릿 (Bun + Vite)",
  "version": "1.0.0",
  "bun-create": {
    "preinstall": [],
    "postinstall": ["rimraf -rf .git"],
    "start": "bun run dev"
  },
  "scripts": {
    "dev": "bunx --bun vite",
    "prod": "bun vite build",
    "preview": "bun vite preview",
    "format:check": "prettier --check --cache \"src/**/*.{js,jsx,json}\"",
    "format:fix": "prettier --write --cache --list-different \"src/**/*.{js,jsx,json}\"",
    "lint:check": "eslint \"src/**/*.{js,jsx,json}\" --cache --cache-location ./node_modules/.cache/eslint/.eslintcache",
    "lint:fix": "bun lint:check --fix",
    "clean": "rimraf -rf dist"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "terser": "^5.43.1",
    "vite": "^7.1.2",
    "vite-plugin-compression": "^0.5.1"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}